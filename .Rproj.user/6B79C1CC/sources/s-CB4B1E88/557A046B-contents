A_salary <- c(25, 28, 50, 60, 30, 35, 40, 70, 40, 70, 40, 100, 30, 30)
B_salary <- c(20, 40, 25, 25, 35, 25, 20, 10, 55, 65, 100, 100, 150, 300)

# 범위 탐색
range(A_salary)
range(B_salary)

# 최소값, 최대값 탐색
min(A_salary)
max(A_salary)
min(B_salary)
max(B_salary)

# B기업의 최소값, 최대값의 범위가 더 넓음을 알 수 있음
# 극단값만 차이가 나는 것일 수도 있음

# 분산
var(A_salary)
var(B_salary)

# 표준편차
sd(A_salary)
sd(B_salary)

